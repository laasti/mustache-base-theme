<div  class="Form">
    <form action="{{ form.getAction }}" method="{{ form.getMethod }}" {{{ form.getFormAttributes }}} novalidate>
        {{# form.getFields }}
        {{> form-field }}
        {{/ form.getFields }}
        {{# form.getGroups }}
        {{> form-group }}
        {{/ form.getGroups }}
    </form>
</div>